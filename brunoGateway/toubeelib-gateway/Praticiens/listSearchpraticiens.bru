meta {
  name: listSearchpraticiens
  type: http
  seq: 2
}

get {
  url: {{gateway}}/praticiens?specialite=dent
  body: none
  auth: none
}

params:query {
  specialite: dent
  ~prenom: ro
  ~nom: del
  ~adresse: 54
}

assert {
  res.status: eq 200
}

tests {
  test("Devrait uniquement Ãªtre dentiste", function(){
    const data = res.getBody()
    data.forEach((prat) => expect(prat.specialiteLabel).to.equal("Dentiste"))
  })
}
